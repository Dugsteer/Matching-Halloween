{
  "name": "vehicles_pelmanism",
  "version": "1.0.0",
  "description": "",
  "main": "javascript.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/styles.css -w",
    "devserver": "live-server --browser=FIREFOX",
    "start": "npm-run-all --parallel devserver watch:sass",
    "autoprefixer": "postcss css/styles.css --use autoprefixer -d build/",
    "compile": "babel --presets es2015,stage-0 -d build/ javascript.js",
    "minify": "uglifyjs javascript.js --compress --mangle --warn --output build/scripts.min.js",
    "minifycss": "minify css/styles.css --output build/stylesmin.css",
    "build": "npm-run-all autoprefixer compile"
  },
  "author": "D Steer",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.8.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "concat": "^1.0.3",
    "live-server": "^1.2.1",
    "minify": "^5.1.1",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "uglify-js": "^3.10.1"
  },
  "dependencies": {}
}
